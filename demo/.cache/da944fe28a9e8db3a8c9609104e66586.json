            setTimeout(() => {\r\n                window.location.href = location.hash\r\n            }, 0);\r\n        }\r\n    }\r\n    render() {\r\n        const state = this.state\r\n        return (\r\n            <div>\r\n                {\r\n                    state.indexPage !== 1 && <Header></Header>\r\n                }\r\n                <CSSTransitionGroup\r\n                transitionName=\"route\"\r\n                transitionEnterTimeout={1}\r\n                transitionLeaveTimeout={3000}>\r\n                    {\r\n                        state.indexPage === 1 ? <Home key={1}></Home> : <Index key={2}></Index>\r\n                    }\r\n                </CSSTransitionGroup>\r\n                {\r\n                    state.indexPage === 1 && <Go letGo={this.letGo.bind(this)}></Go>\r\n                }\r\n            </div>\r\n        )\r\n    }\r\n}\r\n\r\nexport default App\r\n\r\nReactDOM.render(<App />, document.getElementById('app'))"},"lineCount":null}},"hash":"1a20a8d2c2c9b321429aa5316e269867","cacheData":{"env":{}}}